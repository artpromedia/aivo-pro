# District Detection & Analytics Service Configuration

# Service
SERVICE_NAME=district-analytics-svc
VERSION=1.0.0
DEBUG=false
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql+asyncpg://aivo:aivopass@localhost:5432/aivo_analytics

# Redis
REDIS_URL=redis://localhost:6379/8

# External APIs (Optional)
NCES_API_KEY=your_nces_api_key_here
NCES_API_URL=https://nces.ed.gov/api/v1

# Geocoding
NOMINATIM_USER_AGENT=aivo-platform
ZIP_API_URL=https://api.zippopotam.us

# Analytics Configuration
DEFAULT_DATE_RANGE_DAYS=30
MAX_DATA_POINTS=10000
CACHE_TTL_SECONDS=300

# Benchmarking
ENABLE_BENCHMARKS=true
BENCHMARK_UPDATE_HOURS=24
