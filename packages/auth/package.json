{
  "name": "@aivo/auth",
  "version": "1.0.0",
  "description": "Advanced authentication with biometric, MFA, and SSO support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint . --ext ts,tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aivo/types": "workspace:*",
    "@aivo/ui": "workspace:*",
    "@aivo/utils": "workspace:*",
    "@hookform/resolvers": "^3.10.0",
    "axios": "^1.13.1",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.454.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^6.28.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}