{
  "name": "@aivo/visualizations",
  "type": "module",
  "version": "1.0.0",
  "description": "Interactive D3.js data visualizations for AIVO Learning Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^19.0.0",
    "d3": "^7.8.5",
    "d3-selection": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-axis": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-array": "^3.2.4",
    "d3-hierarchy": "^3.1.2",
    "d3-force": "^3.0.0",
    "d3-transition": "^3.0.1",
    "d3-ease": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "framer-motion": "^11.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/d3": "^7.4.3",
    "typescript": "^5.6.2",
    "tsup": "^8.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}